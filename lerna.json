{
  "version": "0.2.2",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "packages": ["packages/*"],
  "sort": [
    "@nuz/shared",
    "@nuz/utils",
    "@nuz/core",
    "@nuz/cli",
    "@nuz/registry"
  ],
  "command": {
    "version": {
      "allowBranch": ["master"],
      "message": "chore(release): %s"
    }
  }
}
